pipeline {
    agent any
    
   
     
    stages {
      stage('checkout') {
           steps {
             
                git branch: 'master', url: 'https://github.com/akhiljosef/CI-usingAnsible.git'
             
          }
        }
       stage('Ansible Init') {
            steps {
              sh 'ansible --version'
            }
       }
       stage('Execute Maven') {
           steps {
             
                sh 'mvn package'             
          }
        }

    }
}